<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

       <servlet>
        <servlet-name>url1</servlet-name>
        <servlet-class>urls1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>url2</servlet-name>
        <servlet-class>urls2</servlet-class>
    </servlet>
    
   
    <servlet-mapping>
        <servlet-name>url1</servlet-name>
        <url-pattern>/servlet/urls1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>url2</servlet-name>
        <url-pattern>/servlet/urls2</url-pattern>
    </servlet-mapping>
    
</web-app>
